{
  "name": "vue3-html2canvas",
  "author": "Novout",
  "license": "MIT",
  "version": "0.0.3",
  "description": "A html2canvas plugin for Vue 3",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/plugpogpag/vue3-html2canvas",
  "scripts": {
    "build": "tsup",
    "patch": "generi log patch",
    "minor": "generi log minor",
    "major": "generi log major",
    "dev": "yarn build && vite",
    "demo": "yarn build && vite build && vite --port 5000 preview",
    "prepublish": "yarn build"
  },
  "keywords": [
    "vue",
    "html2canvas"
  ],
  "dependencies": {
    "compression-webpack-plugin": "^10.0.0",
    "html2canvas": "^1.4.1",
    "regenerator-runtime": "^0.14.0",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vue-demi": "latest",
    "vue-html2canvas": "^0.0.4",
    "vue3htmltocanvas": "^0.0.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.15",
    "@types/node": "^14.18.36",
    "@vitejs/plugin-vue": "^2.3.4",
    "generi": "1.2.0",
    "tsup": "6.7.0",
    "typescript": "^4.9.5",
    "vite": "4.1.1",
    "vue": "^3.2.47"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">= 14"
  }
}
